<style>
	.video-fluid {
		width: 100%;
		height: auto;
	}
</style>

<div class="container-fluid mt-5">
	<div class="content-wrapper">
		<div class="container-fluid mt-3">
			<!-- Breadcrumbs-->
			<ol class="breadcrumb">
				<li class="breadcrumb-item">
					<a href="#">Dashboard</a>
				</li>
