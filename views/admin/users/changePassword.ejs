<div class="content-wrapper mt-2">
	<div class="row mt-5">
		<div class="col-lg-12">
			<ol class="breadcrumb">
				<li class="breadcrumb-item">
					<a href="#">Dashboard</a>
				</li>
				<li class="breadcrumb-item active">Manage Password</li>
			</ol>
		</div>

		<div class="col-lg-12">
			<%- include('../../partials/adminMessages') %>
			<h3>Current Password</h3>
			<hr />
			<form action="/admin/user/password" method="POST">
				<div class="form-group">
					<label for="title">Enter Current Password:</label>
					<input
						type="password"
						class="form-control"
						name="password"
						required
					/>
				</div>

				<button type="submit" class="btn btn-primary mb-3 mt-2">
					Submit
				</button>
			</form>
		</div>
	</div>
</div>
